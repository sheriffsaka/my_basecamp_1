<td>
    <% link_to user.email, user %>
</td>
<td>
    <% form_for(user) do |f| %>
        <% f.select(:role, User.roles.keys.map {|role|[role.titleize,role]}) %>
        <td>
            <button><%= f.submit  'Change Role'%></button>
        </td>
    <% end %>
</td>
<td>
    <%= link_to("Delete User", user_path(user), :turbo_data =>{:confirm => "Are you sure"}, :turbo_method => :delete, :class => 'button') unless user == current_user %>
</td>